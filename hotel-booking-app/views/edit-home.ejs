<!DOCTYPE html>
<html>
<head>
    <title>Edit Hotels</title>
    <%- include('partials/bootstrap') %>    
</head>
<body>
    <div class="container w-100">
            <h4>Hotel List</h4>
            <table class="table">
                <thead class="thead-light">
                  <tr>
                    <th scope="col">Hotel Name</th>
                    <th scope="col">Location</th>
                    <th scope="col">Room Available/th>
                    <th scope="col">Price</th>
                    <th scope="col">Action</th>
                  </tr>
                </thead>
                <tbody>
                    <% for(let data of datas) { %> 
                        <tr>
                          <td><%=data.name%></td>
                          <td><%=data.location%></td>
                          <td><%=data.roomsavailable%></td>
                          <td><%=data.price%></td>
                          <td><a type="button" class="btn btn-primary" href="/hotels/edit/<%=data.id%>">Edit</a>
                            <a type="button" class="btn btn-primary" href="/hotels/delete/<%=data.id%>">Delete</a></td>
                        </tr>
                    <% } %>
                </tbody>
          </table>
    </div>
</body>
</html>